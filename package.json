    "Operational Transformation",
    "HCI",
    "Collaborative Editing"
  ],
  "scripts": {
    "start": "npm run build && node webstrates.js",
    "build": "NODE_ENV='production' webpack -p",
    "watch": "webpack -w",
    "test": "mocha --bail tests/unit-tests tests/functional-tests"
  },
